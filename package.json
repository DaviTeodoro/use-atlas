{
  "name": "use-atlas",
  "version": "1.0.0",
  "description": "geo hooks",
  "author": "DaviTeodoro",
  "license": "MIT",
  "repository": "DaviTeodoro/use-atlas",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format cjs",
    "start": "microbundle-crl watch --no-compress --format cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "d3": "^6.6.2",
    "deck.gl": "^8.4.11",
    "geo-faker": "^1.0.1",
    "immutable": "^4.0.0-rc.12",
    "probe.gl": "^3.3.1",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.7",
    "worker-loader": "^3.0.8"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "files": [
    "dist"
  ]
}
